{
  "mcpServers": { 
    "playwright": {
        "type": "stdio",
        "command": "npx",
        "args": [
            "@playwright/mcp@latest"
        ],
        "gallery": true
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
          "-y",
          "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "firecrawl": {
      "command": "npx",
      "args": [
        "firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "fc-5a4f57d8156143be9663da00f5e58a07"
      }
    },
    "firecrawl-local": {
      "command": "npx",
      "args": [
        "firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_URL": "http://localhost:3002",
        "FIRECRAWL_API_KEY": "no-key-needed-for-local"
      }
    }
  }
}
